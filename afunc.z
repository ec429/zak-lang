byte sump(byte *a, byte *b)
{
	byte tmp = *a;

	tmp += *b;
	return tmp;
}

/* This version happens to produce better register allocation behaviour */
byte sump2(byte *a, byte *b)
{
	byte pa = *a, pb = *b;

	return pb += pa;
}

byte sumpp(byte **a, byte **b)
{
	byte tmp = **a;

	tmp += **b;
	return tmp;
}
